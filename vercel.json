{
  "version": 2,
  
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/server.js": {
      "includeFiles": "backend/data/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/server"
    }
  ]
}
